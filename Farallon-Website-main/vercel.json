{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "name": "farallon-website",
  "installCommand": "npm install",
  "buildCommand": "bash ../scripts/verify-vercel-target.sh && npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

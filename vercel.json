{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "name": "farallon-website",
  "installCommand": "cd Farallon-Website-main && npm install",
  "buildCommand": "bash scripts/verify-vercel-target.sh && cd Farallon-Website-main && npm run build",
  "outputDirectory": "Farallon-Website-main/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
